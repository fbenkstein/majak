namespace ninja;

table BuildLogEntry {
  /// Name of the output.
  output:string (required);
  /// Hash off the command.
  command_hash:uint64;
  /// Time from the start of the build that this command was started.
  start_time:uint32;
  /// Time from the start of the build that this command finished.
  end_time:uint32;
  /// Timestamp of the output.
  mtime:uint64;
}